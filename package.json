{
	"name": "objection-paginator",
	"version": "0.6.7",
	"description": "Paginated queries for Objection.js",
	"keywords": [
		"objection",
		"pagination",
		"cursor",
		"keyset"
	],
	"author": "Batterii, LLC",
	"license": "MIT",
	"repository": "github:Batterii/objection-paginator",
	"engines": {
		"node": ">=12",
		"npm": ">=5.3"
	},
	"files": [
		"dist",
		"!dist/test-utils/",
		"!dist/**/*.spec.js",
		"!dist/**/*.spec.js.map",
		"!dist/**/*.test.js",
		"!dist/**/*.test.js.map"
	],
	"scripts": {
		"lint": "eslint . --ext .ts",
		"lintf": "eslint . --ext .ts --fix",
		"prebuild": "rm -rf dist",
		"build": "tsc && tsc --project tsconfig-types.json",
		"test": "npm run build && mocha dist/**/*.spec.js dist/**/*.test.js",
		"test:unit": "npm run build && mocha dist/**/*.spec.js",
		"test:integration": "npm run build && mocha dist/**/*.test.js",
		"preversion": "npm run lint && npm run test",
		"prepublishOnly": "git push && git push --tags"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"devDependencies": {
		"@batterii/eslint-config-vurvey": "^0.3.3",
		"@batterii/fake-query": "^2.0.1",
		"@types/chai": "^4.2.22",
		"@types/fs-extra": "^9.0.13",
		"@types/lodash": "^4.14.176",
		"@types/mocha": "^9.0.0",
		"@types/node": "^16.11.7",
		"@types/object-path": "^0.11.1",
		"@types/sinon": "^10.0.6",
		"@types/sinon-chai": "^3.2.5",
		"@typescript-eslint/eslint-plugin": "^5.3.1",
		"@typescript-eslint/parser": "^5.3.1",
		"chai": "^4.3.4",
		"eslint": "^7.32.0",
		"fs-extra": "^9.1.0",
		"knex": "^0.21.21",
		"mocha": "^9.1.3",
		"objection": "^2.2.18",
		"sinon": "^10.0.0",
		"sinon-chai": "^3.7.0",
		"source-map-support": "^0.5.20",
		"sqlite3": "^5.0.2",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"@batterii/encode-object": "^2.1.1",
		"@batterii/errors": "^2.2.0",
		"lodash": "^4.17.21",
		"nani": "^3.2.1",
		"object-path": "^0.11.8"
	},
	"peerDependencies": {
		"knex": "0.20.x || 0.21.x",
		"objection": "2.x"
	}
}
